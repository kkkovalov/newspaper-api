{
    "builds": [
        {
            "src": "ecommerce_api/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@https://ecommerce-api.nyc3.digitaloceanspaces.com/",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ecommerce_api/wsgi.py"
        }
    ]
}